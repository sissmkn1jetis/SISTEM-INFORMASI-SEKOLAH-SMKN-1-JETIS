at-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Data User</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <div class=\"card-tools\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-success mb-2\"\r\n                    @click=\"showModal\"\r\n                  >\r\n                    Tambah User\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                <table id=\"example2\" class=\"table table-bordered table-hover\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th>No</th>\r\n                      <th>Username</th>\r\n                      <th>Email</th>\r\n                      <th>Role</th>\r\n                      <th>Action</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(user, index) in users\" :key=\"index.id\">\r\n                      <td>{{ ++index }}</td>\r\n                      <td>{{ user.username }}</td>\r\n                      <td>{{ user.email }}</td>\r\n                      <td>{{ user.role }}</td>\r\n                      <td>Edit | Hapus</td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"modalmuncul\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"modalmuncul1\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-lg\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\">Tambah User</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"close\"\r\n                data-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <span aria-hidden=\"true\">&times;</span>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"card card-default\">\r\n                <!-- /.card-header -->\r\n                <!-- form start -->\r\n                <form @submit.prevent=\"simpanData\">\r\n                  <div v-if=\"!successful\">\r\n                    <div class=\"card-body\">\r\n                      <div class=\"form-group\">\r\n                        <label for=\"exampleInputUsername1\">Username</label>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          id=\"exampleInputUsername1\"\r\n                          placeholder=\"Username\"\r\n                          v-model=\"form.username\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"form-group\">\r\n                        <label for=\"exampleInputEmail1\">Email address</label>\r\n                        <input\r\n                          type=\"email\"\r\n                          class=\"form-control\"\r\n                          id=\"exampleInputEmail1\"\r\n                          placeholder=\"Enter email\"\r\n                          v-model=\"form.email\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"form-group\">\r\n                        <label for=\"exampleInputPassword1\">Password</label>\r\n                        <input\r\n                          type=\"password\"\r\n                          class=\"form-control\"\r\n                          id=\"exampleInputPassword1\"\r\n                          placeholder=\"Password\"\r\n                          v-model=\"form.password\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"form-group\">\r\n                        <label for=\"exampleInputPassword1\"\r\n                          >Confirmasi Password</label\r\n                        >\r\n                        <input\r\n                          type=\"password\"\r\n                          class=\"form-control\"\r\n                          id=\"exampleInputPassword1\"\r\n                          placeholder=\"Confirmasi Password\"\r\n                          v-model=\"form.confPassword\"\r\n                        />\r\n                      </div>\r\n\r\n                      <div class=\"form-group\">\r\n                        <label>Role</label>\r\n                        <select\r\n                          class=\"form-control select2\"\r\n                          v-model=\"form.role\"\r\n                        >\r\n                          <option disabled value=\"\">-- Pilih --</option>\r\n                          <option value=\"admin\">Admin</option>\r\n                          <option value=\"guru\">Guru</option>\r\n                          <option value=\"walas\">Walas</option>\r\n                        </select>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"modal-footer\">\r\n                      <button\r\n                        type=\"button\"\r\n                        class=\"btn btn-secondary\"\r\n                        data-dismiss=\"modal\"\r\n                      >\r\n                        Close\r\n                      </button>\r\n                      <button\r\n                        type=\"submit\"\r\n                        class=\"btn btn-primary\"\r\n                        :disabled=\"loading\"\r\n                      >\r\n                        <span\r\n                          v-show=\"loading\"\r\n                          class=\"spinner-border spinner-border-sm\"\r\n                        ></span>\r\n                        Simpan\r\n                      </button>\r\n                    </div>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport User from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: {},\r\n      form: {\r\n        id: \"\",\r\n        username: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n        confPassword: \"\",\r\n        role: \"\",\r\n      },\r\n      successful: false,\r\n      loading: false,\r\n      message: \"\",\r\n      // submitted: false,\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getdata();\r\n  },\r\n  methods: {\r\n    showModal() {\r\n      // this.form.reset();\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n    },\r\n    getdata() {\r\n      User.getUser()\r\n        .then((response) => {\r\n          this.users = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    simpanData() {\r\n      var data = {\r\n        username: this.form.username,\r\n        email: this.form.email,\r\n        password: this.form.password,\r\n        confPassword: this.form.confPassword,\r\n        role: this.form.role,\r\n      };\r\n      this.message = \"\";\r\n      this.successful = false;\r\n      this.loading = true;\r\n      User.create(data).then(\r\n        (data) => {\r\n          this.message = data.message;\r\n          this.successful = true;\r\n          this.loading = false;\r\n        },\r\n        (error) => {\r\n          this.message =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n          this.successful = false;\r\n          this.loading = false;\r\n        }\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}