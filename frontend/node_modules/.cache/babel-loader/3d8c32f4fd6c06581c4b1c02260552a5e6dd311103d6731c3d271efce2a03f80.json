{"ast":null,"code":"import User from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      users: {},\n      form: {\n        id: \"\",\n        username: \"\",\n        email: \"\",\n        password: \"\",\n        confPassword: \"\",\n        role: \"\"\n      },\n      successful: false,\n      loading: false,\n      message: \"\"\n      // submitted: false,\n    };\n  },\n\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getdata();\n  },\n  methods: {\n    showModal() {\n      // this.form.reset();\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    getdata() {\n      User.getUser().then(response => {\n        this.users = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    simpanData() {\n      var data = {\n        username: this.form.username,\n        email: this.form.email,\n        password: this.form.password,\n        confPassword: this.form.confPassword,\n        role: this.form.role\n      };\n      this.message = \"\";\n      this.successful = false;\n      this.loading = true;\n      User.create(data).then(data => {\n        this.message = data.message;\n        this.successful = true;\n        this.loading = false;\n      }, error => {\n        this.message = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n        this.successful = false;\n        this.loading = false;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAkLA,OAAOA,IAAG,MAAO,6BAA6B;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC;MACTC,IAAI,EAAE;QACJC,EAAE,EAAE,EAAE;QACNC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE,KAAK;MACjBC,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE;MACT;IACF,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EAC