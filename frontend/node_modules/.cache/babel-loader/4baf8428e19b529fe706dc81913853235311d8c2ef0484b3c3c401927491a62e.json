               >\r\n                  Ubah Pengguna\r\n                </h5>\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"close\"\r\n                  data-dismiss=\"modal\"\r\n                  aria-label=\"Close\"\r\n                >\r\n                  <span aria-hidden=\"true\">&times;</span>\r\n                </button>\r\n              </div>\r\n              <form @submit.prevent=\"statusmodal ? ubahData() : simpanData()\">\r\n                <div class=\"modal-body\">\r\n                  <div class=\"form-group\">\r\n                    <input\r\n                      type=\"text\"\r\n                      v-model=\"form.name\"\r\n                      class=\"form-control\"\r\n                      placeholder=\"Nama Pengguna\"\r\n                      :class=\"{\r\n                        'is-invalid': form.errors.has('name'),\r\n                      }\"\r\n                    />\r\n                    <has-error :form=\"form\" field=\"name\"></has-error>\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <select\r\n                      class=\"form-control select2\"\r\n                      v-model=\"form.level_id\"\r\n                      :class=\"{\r\n                        'is-invalid': form.errors.has('level_id'),\r\n                      }\"\r\n                    >\r\n                      <option value>Pilih Level</option>\r\n                      <option\r\n                        v-for=\"item in levels\"\r\n                        :key=\"item.id\"\r\n                        :value=\"item.id\"\r\n                      >\r\n                        {{ item.namalevel }}\r\n                      </option>\r\n                    </select>\r\n                    <has-error :form=\"form\" field=\"level_id\"></has-error>\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <input\r\n                      type=\"text\"\r\n                      v-model=\"form.email\"\r\n                      class=\"form-control\"\r\n                      :class=\"{\r\n                        'is-invalid': form.errors.has('email'),\r\n                      }\"\r\n                      placeholder=\"Email\"\r\n                    />\r\n                    <has-error :form=\"form\" field=\"email\"></has-error>\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <input\r\n                      type=\"password\"\r\n                      v-model=\"form.password\"\r\n                      class=\"form-control\"\r\n                      :class=\"{\r\n                        'is-invalid': form.errors.has('password'),\r\n                      }\"\r\n                      placeholder=\"Password\"\r\n                    />\r\n                    <has-error :form=\"form\" field=\"password\"></has-error>\r\n                  </div>\r\n                </div>\r\n                <div class=\"modal-footer\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-secondary\"\r\n                    data-dismiss=\"modal\"\r\n                  >\r\n                    Close\r\n                  </button>\r\n                  <button\r\n                    type=\"submit\"\r\n                    class=\"btn btn-primary\"\r\n                    :disabled=\"disabled\"\r\n                    v-show=\"!statusmodal\"\r\n                  >\r\n                    <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                    Simpan\r\n                  </button>\r\n                  <button\r\n                    type=\"submit\"\r\n                    class=\"btn btn-success\"\r\n                    :disabled=\"disabled\"\r\n                    v-show=\"statusmodal\"\r\n                  >\r\n                    <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                    Ubah\r\n                  </button>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- /Modal -->\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import User from \"../../services/user.service\";\r\n// export default {\r\n//   data() {\r\n//     return {\r\n//       users: {},\r\n//       role: {},\r\n//       form: {\r\n//         id: \"\",\r\n//         username: \"\",\r\n//         email: \"\",\r\n//         password: \"\",\r\n//         confPassword: \"\",\r\n//         roleId: \"\",\r\n//       },\r\n//       submitted: false,\r\n//     };\r\n//   },\r\n//   computed: {\r\n//     currentUser() {\r\n//       return this.$store.state.auth.user;\r\n//     },\r\n//   },\r\n//   created() {\r\n//     this.getdata();\r\n//   },\r\n//   methods: {\r\n//     showModal() {\r\n//       $(\"#modalmuncul\").modal(\"show\");\r\n//     },\r\n//     getdata() {\r\n//       User.getUser()\r\n//         .then((response) => {\r\n//           this.users = response.data;\r\n//         })\r\n//         .catch((e) => {\r\n//           console.log(e);\r\n//         });\r\n//       User.getRole()\r\n//         .then((response) => {\r\n//           this.role = response.data;\r\n//         })\r\n//         .catch((e) => {\r\n//           console.log(e);\r\n//         });\r\n//     },\r\n//     simpanData() {\r\n//       var data = {\r\n//         username: this.form.username,\r\n//         email: this.form.email,\r\n//         password: this.form.password,\r\n//         confPassword: this.form.confPassword,\r\n//         roleId: this.form.roleId,\r\n//       };\r\n//       User.create(data).then((response) => {\r\n//         this.form.id = response.data.id;\r\n//         console.log(response.data);\r\n//         this.submitted = true;\r\n//         $(\"#modalmuncul\").modal(\"hide\");\r\n//       });\r\n//     },\r\n//   },\r\n// };\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      disabled: false,\r\n      role: {},\r\n      users: {},\r\n      statusmodal: false,\r\n      form: new Form({\r\n        id: \"\",\r\n        username: \"\",\r\n        password: \"\",\r\n        confpassword: \"\",\r\n        roleId: \"\",\r\n      }),\r\n    };\r\n  },\r\n  methods: {\r\n    showModal() {\r\n      this.statusmodal = false;\r\n      this.form.reset();\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n    },\r\n    showModalEdit(item) {\r\n      this.statusmodal = true;\r\n      this.form.reset();\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n      this.form.fill(item);\r\n    },\r\n    loadData() {\r\n      this.$Progress.start();\r\n      axios.get(\"/test/userrole\").then(({ data }) => (this.role = data));\r\n      axios.get(\"/test/user\").then(({ data }) => (this.users = data));\r\n      this.$Progress.finish();\r\n    },\r\n    simpanData() {\r\n      this.$Progress.start();\r\n      this.loading = true;\r\n      this.disabled = true;\r\n      this.form\r\n        .post(\"/test/user\")\r\n        .then(() => {\r\n          Fire.$emit(\"refreshData\");\r\n          $(\"#modalmuncul\").modal(\"hide\");\r\n          Toast.fire({\r\n            icon: \"success\",\r\n            title: \"Data Berhasil Tersimpan\",\r\n          });\r\n          this.$Progress.finish();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n        })\r\n        .catch(() => {\r\n          this.$Progress.fail();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n        });\r\n    },\r\n    ubahData() {\r\n      this.$Progress.start();\r\n      this.loading = true;\r\n      this.disabled = true;\r\n      this.form\r\n        .put(\"/test/user\" + this.form.id)\r\n        .then(() => {\r\n          Fire.$emit(\"refreshData\");\r\n          $(\"#modalmuncul\").modal(\"hide\");\r\n          Toast.fire({\r\n            icon: \"success\",\r\n            title: \"Data Berhasil Terupdate\",\r\n 