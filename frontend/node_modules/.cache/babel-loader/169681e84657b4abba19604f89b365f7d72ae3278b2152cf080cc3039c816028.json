form.reset();\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    showModalEdit(item) {\n      this.statusmodal = true;\n      this.form.reset();\n      $(\"#modalmuncul\").modal(\"show\");\n      this.form.fill(item);\n    },\n    loadData() {\n      this.$Progress.start();\n      axios.get(\"/test/userrole\").then(({\n        data\n      }) => this.role = data);\n      axios.get(\"/test/user\").then(({\n        data\n      }) => this.users = data);\n      this.$Progress.finish();\n    },\n    simpanData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      this.form.post(\"/test/user\").then(() => {\n        Fire.$emit(\"refreshData\");\n        $(\"#modalmuncul\").modal(\"hide\");\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Tersimpan\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(() => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n      });\n    },\n    ubahData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      this.form.put(\"/test/user\" + this.form.id).then(() => {\n        Fire.$emit(\"refreshData\");\n        $(\"#modalmuncul\").modal(\"hide\");\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Terupdate\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(() => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n      });\n    },\n    deleteData(id) {\n      Swal.fire({\n        title: \"Anda Yakin Ingin Menghapus Data Ini ?\",\n        text: \"Klik Batal Untuk Membatalkan Penghapusan\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Hapus\"\n      }).then(result => {\n        if (result.value) {\n          this.form.delete(\"/test/user\" + id).then(() => {\n            Swal.fire(\"Terhapus\", \"Data Anda Sudah Tehapus\", \"success\");\n            Fire.$emit(\"refreshData\");\n          }).catch(() => {\n            Swal.fire(\"Gagal\", \"Data Gagal Terhapus\", \"warning\");\n          });\n        }\n      });\n    }\n  },\n  created() {\n    this.loadData();\n    Fire.$on(\"refreshData\", () => {\n      this.loadData();\n    });\n  }\n};","map":{"version":3,"mappings":"AAwKA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,KAAK;MACfC,IAAI,EAAE,CAAC,CAAC;MACRC,KAAK,EAAE,CAAC,CAAC;MACTC,WAAW,EAAE,KAAK;MAClBC,IAAI,EAAE,IAAIC,IAAI,CAAC;QACbC,EAAE,EAAE,EAAE;QACNC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,MAAM,EAAE;MACV,CAAC;IACH,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IAAI,CAACT,WAAU,GAAI,KAAK;MACxB,IAAI,CAACC,IAAI,CAACS,KAAK,EAAE;MACjBC,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IACjC,CAAC;IACDC,aAAa,CAACC,IAAI,EAAE;MAClB,IAAI,CAACd,WAAU,GAAI,IAAI;MACvB,IAAI,CAACC,IAAI,CAACS,KAAK,EAAE;MACjBC,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;MAC/B,IAAI,CAACX,IAAI,CAACc,IAAI,CAACD,IAAI,CAAC;IACtB,CAAC;IACDE,QAAQ,GAAG;MACT,IAAI,CAACC,SAAS,CAACC,KAAK,EAAE;MACtBC,KAAK,CAACC,GAAG,CAAC,gBAAgB,CAAC,CAACC,IAAI,CAAC,CAAC;QAAE1B;MAAK,CAAC,KAAM,IAAI,CAACG,IAAG,GAAIH,IAAK,CAAC;MAClEwB,KAAK,CAACC,GAAG,CAAC,YAAY,CAAC,CAACC,IAAI,CAAC,CAAC;QAAE1B;MAAK,CAAC,KAAM,IAAI,CAACI,KAAI,GAAIJ,IAAK,CAAC;MAC/D,IAAI,CAACsB,SAAS,CAACK,MAAM,EAAE;IACzB,CAAC;IACDC,UAAU,GAAG;MACX,IAAI,CAACN,SAAS,CAACC,KAAK,EAAE;MACtB,IAAI,CAACtB,OAAM,GAAI,IAAI;MACnB,IAA